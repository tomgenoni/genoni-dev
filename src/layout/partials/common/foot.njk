{# Load any "post.fm.extra" CSS and JS files listed in the md files. #}
{% if post.fm.extra %}
  {% for css in post.fm.extra.css %}
    <link rel="stylesheet" href="{{ css }}" type="text/css" media="print" onload="this.media='all'"/>
  {% endfor %}
  {% for js in post.fm.extra.js %}
    <script async src="{{ js }}"></script>
  {% endfor %}
{% endif %}
<script defer src="/asset/js/instantpage.js" type="module"></script></body></html>